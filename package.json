{
    "name": "pipes-language-support",
    "displayName": "PIPES Language Support",
    "description": "",
    "publisher": "carterisonline",
    "repository": {
        "type": "git",
        "url": "https://github.com/carterisonline/pipes-language-support.git"
    },
    "version": "0.1.2",
    "engines": {
        "vscode": "^1.37.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "icons/pipes-logo.png",
    "contributes": {
        "languages": [{
            "id": "pipes",
            "aliases": ["PIPES", "pipes"],
            "extensions": [".pi"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pipes",
            "scopeName": "source.pipes",
            "path": "./syntaxes/pipes.tmLanguage.json"
        }]
    }
}
